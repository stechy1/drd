<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml/1" alignment="CENTER" prefHeight="400.0" prefWidth="600.0"
  spacing="16" xmlns="http://javafx.com/javafx/8.0.112"
  fx:controller="cz.stechy.drd.controller.bestiary.edit.BestiaryEditOtherController">
  <Hyperlink onAction="#handleShowVulnerabilityPopup" text="%drd_bestiary_vulnerability"
    fx:id="linkVulnerability"/>
  <JFXTextField labelFloat="true" maxWidth="150.0" promptText="%drd_bestiary_mobility"
    fx:id="txtMobility"/>
  <JFXTextField labelFloat="true" maxWidth="150.0" promptText="%drd_bestiary_perservance"
    fx:id="txtPerservance"/>
  <JFXTextField labelFloat="true" maxWidth="150.0" promptText="%drd_bestiary_control_ability"
    fx:id="txtControlAbility"/>
  <JFXTextField labelFloat="true" maxWidth="150.0" promptText="%drd_bestiary_basic_power_of_mind"
    fx:id="txtBasicPowerOfMind"/>
  <JFXTextField labelFloat="true" maxWidth="150.0" promptText="%drd_bestiary_domestication"
    fx:id="txtDomestication"/>
</VBox>
